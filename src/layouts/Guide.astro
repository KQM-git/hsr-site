---
import type MDPost from "types/md-post";
import BaseLayout from "./BaseLayout.astro";
export interface Props extends MDPost {}

const { frontmatter: post, url } = Astro.props;
---

<BaseLayout title={post.title} description={post.description}>
  <main class="flex items-center flex-col">
    <article class="card-bg">
      <slot />
    </article>
  </main>
</BaseLayout>
